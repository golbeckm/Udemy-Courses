
public class WhileLoops {

    public static void main(String[] args) {
        int rows = 5, i = 5, j = 1;

        while (j <= rows) {
            while (j <= i) {
                System.out.print("*");
                j++;
            }
            System.out.print("\n");
            i++;
            j = 1;
        }
    }
}